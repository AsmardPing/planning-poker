    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.6.2",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.26.0",
    "@aws-cdk/aws-dynamodb": "^1.26.0",
    "@aws-cdk/aws-iam": "^1.26.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.26.0",
    "@aws-cdk/core": "^1.26.0",
    "@types/cucumber": "^6.0.1",
    "aws-sdk": "^2.630.0",
