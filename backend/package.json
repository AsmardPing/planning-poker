{
  "name": "infrastructure",
  "version": "0.1.0",
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "DEBUG=testcontainers jest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "feature": "cucumber-js --require-module ts-node/register/transpile-only --require src/__features__/support/setup.ts --require 'src/__features__/support/*Steps.ts'",
    "features": "npm run feature src/__features__/",
    "cdk": "cdk",
    "synth": "cdk synth --profile planning-poker",
    "bootstrap": "cdk bootstrap --profile planning-poker",
    "deploy": "cdk deploy --profile planning-poker"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.31.0",
    "@types/aws-lambda": "^8.10.47",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^25.1.3",
    "@types/lambda-log": "^2.2.0",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "aws-cdk": "^1.31.0",
    "cucumber": "^6.0.5",
    "eslint": "^6.8.0",
    "jest": "^24",
    "testcontainers": "^2.8.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.26.0",
    "@aws-cdk/aws-dynamodb": "^1.26.0",
    "@aws-cdk/aws-events": "^1.32.2",
    "@aws-cdk/aws-events-targets": "^1.32.2",
    "@aws-cdk/aws-iam": "^1.26.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.26.0",
    "@aws-cdk/core": "^1.26.0",
    "aws-sdk": "^2.630.0",
    "aws-xray-sdk": "^2.5.0",
    "lambda-log": "^2.3.1",
    "parcel-bundler": "^1",
    "source-map-support": "^0.5.16"
  }
}
